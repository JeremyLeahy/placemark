{{#each coffeeShops}}
    <div class="box box-link-hover-shadow">
        <h2 class="title">
            {{title}}
        </h2>
        <a href="/coffeeshop/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open" color="blue"></i>
      </span>
        </a>
        <a href="/dashboard/deletecoffeeshop/{{_id}}" class="button">
            <i class="fas fa-trash" color="red"></i>
        </a>
    </div>
{{/each}}
